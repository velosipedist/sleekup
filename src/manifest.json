{
  "manifest_version": 3,
  "name": "Sleekup",
  "version": "0.0.3",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://app.clickup.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.clickup.com/*"
      ],
      "css": [],
      "js": [
        "page.js"
      ]
    }
  ]
}
